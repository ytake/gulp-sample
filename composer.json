{
    "name": "ytake/gulp-tutorial",
    "description": "gulp-tutorial",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "yuuki takezawa",
            "email": "yuuki.takezawa@comnect.jp.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "iono/container": "0.*",
        "nikic/fast-route": "*",
        "symfony/http-foundation": "*"
    },
    "require-dev": {
        "symfony/var-dumper": "*",
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.*",
        "phpspec/phpspec": "~2.1",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Acme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "_Test\\": [
                "tests/"
            ]
        }
    }
}
